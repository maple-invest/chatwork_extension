{
  "name": "chatwork表示スッキリツール",
  "description": "沢山の表示でメッセージが流れてしまう現象を解決します",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    //Twitter読み込み時
    {
      "matches": ["https://www.chatwork.com/*"],
      "js"     : ["jquery-3.5.1.min.js","background.js"]
    }
  ],
  "manifest_version": 2
}
